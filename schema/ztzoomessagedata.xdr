<Schema name="ZTZooMessageData">
	<ElementType name="ZTZooMessageData">
		<extends type="BFXMLObj"/>
		
		<AttributeType name="entityptr">
			<datatype dt:type="bin.hex" bf:pointer="BFGEntity"/>
		</AttributeType>
		
		<attribute name="display" dt:type="string" default="" />
		<attribute name="priority" dt:type="int" default="" />
		<attribute name="timeout" dt:type="float" default="" />
		<attribute name="target" type="entityptr" default=0 />
	</ElementType>
</Schema>