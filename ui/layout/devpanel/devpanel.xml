      <UIListBox name="tokenlist" rows="-1">
         <UIRegion x="720" y="140" w="300" h="768" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIAspect alwayshit="never">
