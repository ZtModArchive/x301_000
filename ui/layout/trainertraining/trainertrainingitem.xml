<UILayout name="Trainer Training Entry">
   <UIRegion x="4" y="0" w="60" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
   <UIState active="normal" visible="normal"/>
   <UIAspect>
      <default image=""/>
   </UIAspect>
   <children>
      <UIToggleButton name="person training trick button" sticky="0" toggle="1">
         <UIRegion x="0" y="0" w="55" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIHelpInfo name="" short="" long="" help="" ids=""/>
         <UIAspect>
            <default image="">
               <BFRect x="0" y="0" w="64" h="64"/>
               <BFColor r="255" g="255" b="255" a="255"/>
            </default>
            <standard>
               <normal>
                  <BFRect x="0" y="0" w="64" h="64"/>
               </normal>
               <highlighted>
                  <BFRect x="0" y="64" w="64" h="64"/>
               </highlighted>
               <activated>
                  <BFRect x="64" y="0" w="64" h="64"/>
               </activated>
               <disabled>
                  <BFRect x="64" y="64" w="64" h="64"/>
               </disabled>
            </standard>
            <alternate>
               <normal>
                  <BFRect x="64" y="128" w="64" h="64"/>
               </normal>
               <highlighted>
                  <BFRect x="0" y="192" w="64" h="64"/>
               </highlighted>
               <activated>
                  <BFRect x="64" y="128" w="64" h="64"/>
               </activated>
               <disabled>
                  <BFRect x="64" y="192" w="64" h="64"/>
               </disabled>
            </alternate>
         </UIAspect>
      </UIToggleButton>
      <UILayout name="TrainingComplete TrickImage Layout">
         <UIRegion x="0" y="0" w="55" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIAspect>
            <default image=""/>
         </UIAspect>
         <children>
            <UIStatic name="TrainingComplete TrickImage">
               <UIRegion x="0" y="0" w="55" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
               <UIState active="normal" visible="normal"/>
               <UIHelpInfo name="" short="" long="" help="" ids=""/>
               <UIAspect>
                  <default image="">
                     <BFRect x="0" y="0" w="64" h="64"/>
                     <BFColor r="255" g="255" b="255" a="255"/>
                  </default>
               </UIAspect>
            </UIStatic>
            <UIStatic name="completeimage">
               <UIRegion x="30" y="30" w="25" h="25" xalign="min" yalign="min" walign="len" halign="len"/>
               <UIState active="normal" visible="normal"/>
               <UIAspect>
                  <default image="ui\shared\icon_fullytrained.dds">
                     <BFRect x="0" y="0" w="64" h="64"/>
                     <BFColor r="255" g="255" b="255" a="175"/>
                  </default>
               </UIAspect>
            </UIStatic>
         </children>
      </UILayout>
      <UIStatic name="trick icon locked">
         <UIRegion x="7" y="5" w="40" h="40" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIAspect alwayshit="never">
            <default image="ui/scenario/button_lock.dds">
               <BFRect x="0" y="0" w="64" h="64"/>
               <BFColor r="255" g="255" b="255" a="128"/>
            </default>
         </UIAspect>
      </UIStatic>
      <UIStatic name="trick score layer 1">
         <UIRegion x="0" y="0" w="13" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="normal"/>
         <UIAspect alwayshit="never">
            <default image="ui\tricks\trick_sequencer_asset.dds">
               <BFRect x="0" y="85" w="14" h="72"/>
               <BFColor r="255" g="255" b="255" a="255"/>
            </default>
         </UIAspect>
      </UIStatic>
      <UIStatic name="trick score layer 2">
         <UIRegion x="0" y="0" w="11" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIAspect alwayshit="never">
            <default image="ui\tricks\trick_sequencer_asset.dds">
               <BFRect x="16" y="85" w="11" h="72"/>
               <BFColor r="255" g="255" b="255" a="255"/>
            </default>
         </UIAspect>
      </UIStatic>
      <UIStatic name="trick score layer 3">
         <UIRegion x="2" y="0" w="11" h="55" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState active="normal" visible="hidden"/>
         <UIAspect alwayshit="never">
            <default image="ui\tricks\trick_sequencer_asset.dds">
               <BFRect x="31" y="85" w="11" h="72"/>
               <BFColor r="255" g="255" b="255" a="255"/>
            </default>
         </UIAspect>
      </UIStatic>
   </children>
</UILayout>
