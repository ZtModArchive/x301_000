<UILayout name="icon item">
   <UIRegion x="0" y="0" w="42" h="42" xalign="min" yalign="min" walign="len" halign="len"/>
   <UIState active="normal" visible="normal"/>
   <UIHelpInfo name="" short="" long="" help="" ids=""/>
   <UIAspect alwayshit="true">
      <default/>
   </UIAspect>
   <children>
      <UIMultiIcon name="icon background">
         <UIRegion x="1" y="1" w="-1" h="-1" xalign="min" yalign="min" walign="max" halign="max"/>
         <UIState active="normal" visible="normal"/>
         <UIAspect alwayshit="never">
            <default/>
         </UIAspect>
         <images>
            <entry key="animal_hunger" image="ui/icon_needs/all_hunger.dds"/>
            <entry key="animal_thirst" image="ui/icon_needs/all_thirst.dds"/>
            <entry key="animal_environment" image="ui/icon_needs/animal_biome.dds"/>
            <entry key="animal_rest" image="ui/icon_needs/animal_rest.dds"/>
            <entry key="animal_exercise" image="ui/icon_needs/animal_exercise.dds"/>
            <entry key="animal_shelter" image="ui/icon_needs/animal_shelter.dds"/>
            <entry key="animal_privacy" image="ui/icon_needs/animal_privacy.dds"/>
            <entry key="animal_hygiene" image="ui/icon_needs/animal_hygiene.dds"/>
            <entry key="animal_social" image="ui/icon_needs/animal_social.dds"/>
            <entry key="animal_stimulation" image="ui/icon_needs/animal_stimulation.dds"/>
            <entry key="animal_happy" image="ui/icon_needs/all_good.dds"/>
            <entry key="animal_not_happy" image="ui/icon_needs/all_bad.dds"/>
            <entry key="animal_pregnant" image="ui/icon_needs/animal_pregnant.dds"/>
            <entry key="animal_not_pregnant" image="ui/icon_needs/animal_pregnant_not.dds"/>
            <entry key="animal_eggtimer" image="ui/infopan/animal_eggtimer.dds"/>
            <entry key="animal_sick" image="ui/icon_needs/animal_sick.dds"/>
            <entry key="animal_well" image="ui/icon_needs/animal_sick_sick.dds"/>
            <entry key="guest_happy" image="ui/icon_needs/all_good.dds"/>
            <entry key="guest_unhappy" image="ui/icon_needs/all_bad.dds"/>
            <entry key="guest_hunger" image="ui/icon_needs/all_hunger.dds"/>
            <entry key="guest_thirst" image="ui/icon_needs/all_thirst.dds"/>
            <entry key="guest_rest" image="ui/icon_needs/guest_rest.dds"/>
            <entry key="guest_amusement" image="ui/icon_needs/guest_amusement.dds"/>
            <entry key="guest_bathroom" image="ui/icon_needs/guest_bathroom.dds"/>
         </images>
      </UIMultiIcon>
      <UILayout name="foreground container">
         <UIRegion x="3" y="3" w="-3" h="-3" xalign="min" yalign="min" walign="max" halign="max"/>
         <UIState active="normal" visible="normal"/>
         <children>
            <UIStatic name="icon foreground">
               <UIRegion x="0" y="0" w="100" h="100" xalign="% min" yalign="% min" walign="% min" halign="% min"/>
               <UIState active="normal" visible="normal"/>
               <UIAspect alwayshit="never">
                  <default image="ui/shared/white1x1.dds" sound="">
                     <BFRect x="0" y="0" w="1" h="1"/>
                     <BFColor r="255" g="255" b="255" a="255"/>
                  </default>
               </UIAspect>
            </UIStatic>
         </children>
      </UILayout>
   </children>
</UILayout>
