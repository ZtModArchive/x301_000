<UITextEdit templateName="textedit" maxlength="17" changesound="keypress" errorsound="">
   <UIAspect alwayshit="true">
      <default/>
   </UIAspect>
   <cursor>
      <UIStatic>
         <UIRegion x="0" y="0" w="3" h="100"/>
         <UIAspect>
            <default image="ui/shared/white1x1.dds">
               <BFColor r="0" g="0" b="0" a="255"/>
            </default>
            <BFColor r="125" g="125" b="125" a="200"/>
         </UIAspect>
      </UIStatic>
   </cursor>
   <highlight>
      <UIStatic>
         <UIRegion x="0" y="3" w="0" h="-3" xalign="min" yalign="min" walign="len" halign="max"/>
         <UIAspect alwayshit="never">
            <default image="ui/shared/white1x1.dds">
               <BFColor r="64" g="128" b="255" a="128"/>
            </default>
         </UIAspect>
      </UIStatic>
   </highlight>
   <children>
      <UILayout name="edit_back" template="editfield">
         <UIRegion x="0" y="0" w="0" h="0" xalign="min" yalign="min" walign="max" halign="max"/>
         <UIState visible="normal" active="normal"/>
      </UILayout>
   </children>
</UITextEdit>
