<ZT2Strings>
   <locale>
      <!-- Date formats: the month_year format is used for the main game date display /-->
      <month_year>MMM',' 'Year' y</month_year>
      <day_date>ddd'.' MMM d</day_date>
      <month_day_year>MMM d', Year' y</month_day_year>
      <minutes_seconds>m':'ss</minutes_seconds>
      <!-- Currency Symbol: if left empty or commented out in data/config/locale.xml, the default symbol is used -->
      <currency_symbol>$</currency_symbol>
      <!-- Month Abbreviations: if left empty or commented out in data/config/locale.xml, defaults are used -->
      <january>Jan</january>
      <february>Feb</february>
      <march>Mar</march>
      <april>Apr</april>
      <may>May</may>
      <june>Jun</june>
      <july>Jul</july>
      <august>Aug</august>
      <september>Sep</september>
      <october>Oct</october>
      <november>Nov</november>
      <december>Dec</december>
   </locale>
</ZT2Strings>
