               <ZTThoughtInfo locID="guestthoughts:LaughBathroom" priority="0" timeout="30" global="false" useTargetName="true"/>
               <ZTActionInfo locID="guestactions:LaughBathroom" useTargetName="true"/>
            </ZTFeedbackData>
         </ZTBehFeedback>
         <BFBehAnimate targetAnim="Stand_Laugh"/>
         <BFBehFaceTarget/>
